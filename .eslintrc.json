{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "consistent-return": 0,
        "max-len": [1, 110, 4],
        "max-params": ["error", 6],
        "object-curly-spacing": 0,
        "babel/object-curly-spacing": 2
    },
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "babel"
    ]
}
